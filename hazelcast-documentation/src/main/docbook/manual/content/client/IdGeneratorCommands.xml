<sect1 xml:id="IdGeneratorCommands" version='5.0' xmlns='http://docbook.org/ns/docbook'
       xmlns:xi="http://www.w3.org/2001/XInclude"
       xmlns:xlink="http://www.w3.org/1999/xlink"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://docbook.org/ns/docbook http://www.docbook.org/xml/5.0/xsd/docbook.xsd
       http://www.w3.org/1999/xlink http://www.w3.org/1999/xlink.xsd">
    <title>ID Generatpr Commands</title>

    NEWID

    <sect2 xml:id="NEWID" version="5.0" xmlns="http://docbook.org/ns/docbook">
        <title>NEWID</title>

        <para>
            Generates a new cluster wide unique id.
        </para>

        <para>
            <emphasis role="bold">COMMAND:</emphasis>
        </para>

        <programlisting language="java"><![CDATA[
        NEWID <name>
        ]]></programlisting>

        <para>
            <emphasis role="bold">Return value:</emphasis>
        </para>
        <programlisting language="java"><![CDATA[
        OK <newid>
            ]]></programlisting>

        <para>
            Returns the new generated id as long in Java.
        </para>
    </sect2>

</sect1>
